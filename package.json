{
  "name": "peranta",
  "version": "0.2.3",
  "description": "HTTP-like API for inter-process communication (IPC) in javascript",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=production ./node_modules/.bin/mocha --compilers js:babel-core/register",
    "clean_dist": "shx rm -rf ./dist && mkdirp ./dist",
    "copy-pkg": "mkdirp ./dist && node ./scripts/copy_pkg.js",
    "build-cjs": "cross-env BABEL_ENV=commonjs babel src --out-dir dist",
    "build-umd-dev": "NODE_ENV=development webpack --progress --colors",
    "build-umd-prod": "NODE_ENV=production webpack --progress --colors",
    "build": "npm run clean_dist && npm run build-cjs && npm run build-umd-dev && npm run build-umd-prod",
    "pub": "npm run build && npm run copy-pkg && npm publish ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loque/peranta.git"
  },
  "keywords": [
    "express",
    "koa",
    "electron",
    "webworker",
    "ipc",
    "http"
  ],
  "author": "Lucas Soler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loque/peranta/issues"
  },
  "homepage": "https://github.com/loque/peranta#readme",
  "dependencies": {
    "array-flatten": "^2.1.0",
    "object.values": "^1.0.3",
    "path-to-regexp": "^1.7.0",
    "qs": "^6.3.0",
    "url-parse": "^1.1.7",
    "uuid": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "mocha": "^3.1.2",
    "shx": "^0.2.2",
    "webpack": "^1.13.3"
  }
}
